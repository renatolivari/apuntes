ELIMINAR ALGUN OPERADOR !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!

var unObjeto = {izq: 1, der: 2};
console.log(unObjeto.izq);
// → 1
delete unObjeto.izq;
console.log(unObjeto.izq);
// → undefined
console.log("izq" in unObjeto);
// → false
console.log("der" in unObjeto);
// → true

