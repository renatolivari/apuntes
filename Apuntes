var dia1 = {
  prueba: false,
  eventos: ["trabajo", "toqué un arbol", "pizza", "correr",
           "televisión"]
};
console.log(dia1.prueba);
// → false
console.log(dia1.lob);
// → undefined
dia1.lob = false;
console.log(dia1.lob);
// → false

