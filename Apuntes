//RAIZ CUADRADA DE NUMERO
var elNumero = Numero(prompt("Ecriba Numero", ""));
alert("Tú número es la raíz cuadrada de " +
      elNumero * elNumero);

-----------------------------------------------------------------------------------------
TAMAÑO DE ACUERDO AL NUMERO

var num = Number(prompt("Dame un número", "0"));

if (num < 10)
  alert("Chico");
else if (num < 100)
  alert("Mediano");
else
  alert("Grande");
  
-------------------------------------------------------------------------------------------
