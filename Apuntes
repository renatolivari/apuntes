var tono = "claro";
console.log(tono);
// → RESULTADO = claro
tono = "oscuro";
console.log(tono);
// → RESULTADO = oscuro
----------------------------------------------------------------------------------------
var alcuadrado = function(x) {
  return x * x;
};

console.log(alcuadrado(12));
//Respuesta → 144

OPERACION SIMPLE "AL CUADRADO"
-----------------------------------------------------------------------------------------

var hruido =function(){
  console.log("HOLA")
};
hRuido();
// MENSAJE = HOLA

var potencia = function(x,y){
var resultado = 1;
for(var contador=0;contador<y;contador++)
  resultado *= x;
  return resultado;
};
console.log(potencia(2,10));
